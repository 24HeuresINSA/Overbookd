{
  "conventionalCommits.autoCommit": false,
  "conventionalCommits.scopes": [
    "Docker",
    "Front",
    "Back",
    "Config",
    "Log",
    "Planning",
    "QoL",
    "Secu",
    "Soft",
    "User",
    "Auth",
    "triage",
    "CP",
    "FA",
    "release",
    "Location",
    "animation",
    "FT",
    "Availabilities",
    "Affect",
    "trombinoscope"
  ],
  "cSpell.enabled": false,
  "colorize.languages": [
    "javascript",
    "json"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/docker/data/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/docker/data/**": true
  },
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
}