{
  "conventionalCommits.autoCommit": false,
  "conventionalCommits.scopes": [
    "ci",
    "web",
    "api",
    "configuration",
    "catalog",
    "gear-request",
    "permission",
    "assignment",
    "planning",
    "secu",
    "user",
    "authentication",
    "personnal-account",
    "fa",
    "release",
    "signa",
    "ft",
    "volunteer-availability",
    "timeline",
    "list",
    "period",
    "trombinoscope"
  ],
  "cSpell.enabled": false,
  "colorize.languages": [
    "javascript",
    "json"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/docker/data/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/docker/data/**": true
  },
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
}
