{
  "name": "overbookd",
  "version": "1.9.2-rc.0",
  "description": "Frontend pour le logiciel de gestion du festival des 24 heures de l'INSA",
  "author": "Club des 24 heures de l'INSA (https://www.24heures.org/)",
  "homepage": "https://gitlab.com/24-heures-insa/overbookd-mono",
  "license": "Apache-2.0",
  "private": true,
  "bugs": {
    "url": "https://gitlab.com/24-heures-insa/overbookd-mono/-/issues/new",
    "email": "contact-project+24-heures-insa-overbookd-mono-31598236-issue-@incoming.gitlab.com"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/24-heures-insa/overbookd-mono",
    "directory": "frontend"
  },
  "scripts": {
    "dev": "nuxt",
    "dev:host": "nuxt --hostname '0'",
    "build": "./node_modules/.bin/nuxt build",
    "build:ci": "CI=1 npm run build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore . --fix",
    "lint": "npm run lint:js",
    "test": "jest",
    "test:ci": "jest --collectCoverage --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1667386184.dfbbb54",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/pwa": "^3.3.5",
    "@types/jwt-decode": "^3.1.0",
    "core-js": "^3.28.0",
    "csv-parse": "^5.3.5",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.6.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "nuxt": "^2.14.12",
    "nuxt-typed-vuex": "^0.3.1",
    "tiptap-vuetify": "^2.24.0",
    "uuid": "^9.0.0",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vuetify-datetime-picker": "^2.1.1",
    "vuex-module-decorators": "^1.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/color-mode": "^2.1.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "cypress": "^12.6.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^1.1.5",
    "eslint-plugin-vue": "^7.5.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-junit": "^15.0.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "sass-loader": "^10.4.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageReporters": [
      "html",
      "text",
      "text-summary",
      "cobertura"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "jsdom"
  }
}
