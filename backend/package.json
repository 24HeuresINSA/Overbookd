{
  "name": "overbookd",
  "version": "0.24.0-beta",
  "scripts": {
    "build": "./node_modules/.bin/ts-node build.ts",
    "lint:check": "eslint .",
    "lint": "eslint . --fix",
    "pretify:check": "prettier . --check",
    "pretify": "prettier . --write",
    "prepare": "cd .. && husky install",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "jest --runInBand --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --runInBand --coverage",
    "migrate:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/.bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:down": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts",
    "migrate:up": "ts-node node_modules/.bin/migrate-mongo up -f migrate-mongo-config.ts"
  },
  "lint-staged": {
    "@(src|__tests__)/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/server",
    "@src": "dist/"
  },
  "dependencies": {
    "@sentry/node": "^6.9.0",
    "@sentry/tracing": "^6.9.0",
    "@types/migrate-mongo": "^8.1.3",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.4",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "command-line-args": "^5.1.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.13.0",
    "helmet": "^4.6.0",
    "html-to-text": "^8.2.0",
    "http-status-codes": "^2.1.4",
    "ical-generator": "^3.4.2",
    "jet-logger": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.5.1",
    "keycloak-connect": "^14.0.0",
    "migrate-mongo": "^8.2.3",
    "module-alias": "^2.2.2",
    "mongodb": "^4.4.0",
    "mongoose": "^5.13.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemailer": "^6.7.2",
    "qs": "^6.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/command-line-args": "^5.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.12",
    "@types/html-to-text": "^8.1.0",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/luxon": "^2.3.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.33",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "dayjs": "^1.11.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.5.2",
    "factory.ts": "^0.5.2",
    "faker": "^5.5.3",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^11.2.0",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "mongodb-memory-server": "^7.4.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "rrule": "^2.6.9",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5"
  }
}
