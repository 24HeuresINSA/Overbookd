{
  "name": "@overbookd/registration",
  "version": "1.0.0-rc.8",
  "description": "Gestion de l'inscription des utilisateurs pour overbookd",
  "author": "Club des 24 heures de l'INSA (https: //www.24heures.org/)",
  "homepage": "https://gitlab.com/24-heures-insa/overbookd-mono",
  "license": "Apache-2.0",
  "private": true,
  "main": "dist/domains/registration/src/index.js",
  "bugs": {
    "url": "https://gitlab.com/24-heures-insa/overbookd-mono/-/issues/new",
    "email": "contact-project+24-heures-insa-overbookd-mono-31598236-issue-@incoming.gitlab.com"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/24-heures-insa/overbookd-mono",
    "directory": "domains/registration"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "build:ci": "pnpm build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prune": "ts-prune --error --ignore 'index.ts|dist'",
    "release:candidate": "pnpm version prerelease --preid=rc --commit-hooks=false --git-tag-version=false",
    "release:major": "pnpm version major --commit-hooks=false --git-tag-version=false",
    "release:minor": "pnpm version minor --commit-hooks=false --git-tag-version=false",
    "release:patch": "pnpm version patch --commit-hooks=false --git-tag-version=false",
    "test": "vitest --config ../../vitest.config.ts",
    "test:ci": "vitest --config ../../vitest.config.ts --coverage --reporter=junit --watch=false --outputFile=junit.xml"
  },
  "keywords": [
    "registration"
  ],
  "engines": {
    "node": "20.5.1",
    "pnpm": "8.7.1"
  },
  "dependencies": {
    "@overbookd/list": "workspace:1.0.0-rc.2",
    "@overbookd/period": "workspace:^",
    "jsonwebtoken": "9.0.2",
    "jwt-decode": "3.1.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "9.0.2"
  }
}
